<div class="container">
  <%# TODO: 
  1. If user is not signed in - display "Welcome".
  2. If user is signed in then  display all users %>
  <% if @user %>
		<h1>Welcome <%= @user.name %></h1>
	<% else %>
		<h1>Welcome</h1>
	<% end %>

	<% if logged_in? %>
		<ul>
		  <% User.all.each do |user| %>
		  <li><%= user.name %></li>
		  <% end %>
		</ul>
	<% end %>
</div>
